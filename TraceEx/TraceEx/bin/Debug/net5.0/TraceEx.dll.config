<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<sources>
			<source name ="MyTrace" switchValue="All">
				<listeners>
					<add name="consoleLister" type="System.Dianostics.ConsoleTraceListener" />
					<add name="fileLister" type="System.Dianostics.TextWriterTraceListener" initializeData="AppLogs.txt"/>
				</listeners>
			</source>
		</sources>
	</system.diagnostics>
</configuration>