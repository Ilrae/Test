<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<sources>
			<source name="MyTrace" switchValue="All">
				<listeners>
					<add name="fileListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Logs.txt" />
				</listeners>
			</source>
		</sources>
	</system.diagnostics>
</configuration>